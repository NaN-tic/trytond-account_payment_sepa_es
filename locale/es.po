msgctxt ""
"model:ir.message,text:bad_relation_mandate_number_vs_bank_account_numbers"
msgid ""
"The mandate \"%(mandate)s\" for payment \"%(payment)s\" of party "
"\"%(party)s\" with amount \"%(amount)s\" is not for the move line bank "
"accounts \"%(bank_account_numbers)s\"."
msgstr ""
"El mandato \"%(mandate)s\" para el pago \"%(payment)s\" del tercero "
"\"%(party)s\"  con el importe \"%(amount)s\" no es correcto para la cuenta "
"bancaria \"%(bank_account_numbers)s\"."

msgctxt "model:ir.message,text:cancel_with_processing_payments"
msgid ""
"Mandate \"%(mandate)s\" can not be canceled because it is linked to payment "
"\"%(payment)s\" which is still in process."
msgstr ""
"El mandato \"%(mandate)s\" no se puede cancelar porque está vinculado al "
"pago \"%(payment)s\" que aún está en proceso."

msgctxt "model:ir.message,text:canceled_mandate"
msgid ""
"Payment \"%(payment)s\" can not be modified because its mandate "
"\"%(mandate)s\" is canceled."
msgstr ""
"El pago \"%(payment)s\" no puede ser modificado porque su mandato "
"\"%(mandate)s\" está cancelado."

msgctxt "model:ir.message,text:invalid_payment_date"
msgid ""
"Invalid payment date %(payment_date)s for payment \"%(payment)s\". "
"Payment\\'s date must be greather or equal than today."
msgstr ""
"Fecha de pago no vàlida \"%(payment_date)s\". La fecha de pago debe ser "
"mayor o igual a la de hoy."

msgctxt "model:ir.message,text:no_bank_account"
msgid ""
"Payment \"%(payment)s\" of party \"%(party)s\" with amount \"%(amount)s "
"hasn't bank account.\"."
msgstr ""
"Pago \"%(payment)s\" del tercero \"%(party)s\"  con el importe "
"\"%(amount)s\" no tiene cuenta bancaria."

msgctxt "model:ir.message,text:no_creditor_identifier"
msgid "No creditor identifier for party \"%(party)s\"."
msgstr "No hay identificador de acreedor para el tercero \"%(party)s\"."

msgctxt "model:ir.message,text:no_mandate_for_party"
msgid ""
"No valid mandate for payment \"%(payment)s\" of party \"%(party)s\" with "
"amount \"%(amount)s\"."
msgstr ""
"No hay un mandato válido para el pago \"%(payment)s\" del tercero "
"\"%(party)s\" con el importe \"%(amount)s\"."

