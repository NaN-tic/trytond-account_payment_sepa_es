msgctxt "error:account.payment.group:"
msgid ""
"Invalid payment date %s for payment \"%s\". Payment's date must be greather "
"or equal than today."
msgstr ""
"La data de pagament \"%s\" del pagament \"%s\" no és vàlida. La data de "
"pagament ha de ser més gran o igual que avui."

msgctxt "error:account.payment.group:"
msgid "No creditor identifier for party \"%s\"."
msgstr "No s'ha definit l'identificador del creditor per al tercer \"%s\"."

msgctxt "error:account.payment.sepa.mandate:"
msgid ""
"Mandate \"%(mandate)s\" can not be canceled because it is linked to payment "
"\"%(payment)s\" which is still in process."
msgstr ""
"No es pot cancel·lar el mandat \"%(mandate)s\" perquè esta vinculat amb el "
"pagament \"%(payment)s\" que encara esta en procés. "

msgctxt "error:account.payment:"
msgid "Missing IBAN bank account in \"%(party)s\"."
msgstr "Falta el compte bancari IBAN a \"%(party)s\"."

msgctxt "error:account.payment:"
msgid ""
"No valid mandate for payment \"%(payment)s\" of party \"%(party)s\" with "
"amount \"%(amount)s\"."
msgstr ""
"No s'ha trobat un mandat vàlid per al pagament \"%(payment)s\" del tercer "
"\"%(party)s\" amb import \"%(amount)s\"."

msgctxt "error:account.payment:"
msgid ""
"Payment \"%(payment)s\" can not be modified because its mandate "
"\"%(mandate)s\" is canceled."
msgstr ""
"El Pagament \"%(payment)s\" no es pot modificar perquè el seu mandat "
"\"%(mandate)s\" està cancel·lat."

