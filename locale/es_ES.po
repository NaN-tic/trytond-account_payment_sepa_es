msgctxt "error:account.payment.group:"
msgid ""
"Invalid payment date %s for payment \"%s\". Payment's date must be greather "
"or equal than today."
msgstr ""
"La fecha de pago \"%s\" del pago \"%s\" no es válida. La fecha de pago debe "
"ser superior o igual a hoy."

msgctxt "error:account.payment.group:"
msgid "No creditor identifier for party \"%s\"."
msgstr "No se ha definido el identificador del creditor para el tercero \"%s\"."

msgctxt "error:account.payment.sepa.mandate:"
msgid ""
"Mandate \"%(mandate)s\" can not be canceled because it is linked to payment "
"\"%(payment)s\" which is still in process."
msgstr ""
"El mandato \"%(mandate)s\" no se puede cancelar porqué esta vinculado con el"
" pago \"%(payment)s\" que aún esta en proceso."

msgctxt "error:account.payment:"
msgid ""
"No valid mandate for payment \"%(payment)s\" of party \"%(party)s\" with "
"amount \"%(amount)s\"."
msgstr ""
"No se ha encontrado un mandato valido para el pago \"%(payment)s\" del "
"tercero \"%(party)s\" con importe \"%(amount)s\"."

msgctxt "error:account.payment:"
msgid ""
"Payment \"%(payment)s\" can not be modified because its mandate "
"\"%(mandate)s\" is canceled."
msgstr ""
"El pago \"%(payment)s\" no se puede modificar porqué su mandato "
"\"%(mandate)s\" esta cancelado."

msgctxt "error:party.party:"
msgid "Invalid creditor identifier \"%(identifier)s\" for party \"%(party)s\"."
msgstr ""
"El idetnificador de creditor \"%(identifier)s\" para el tercero "
"\"%(party)s\" es inválido."

